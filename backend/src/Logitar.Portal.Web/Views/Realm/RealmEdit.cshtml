@using Logitar.Portal.Core.Realms.Models;

@model RealmModel
@{
  Layout = "_Layout";

  ViewBag.Title = Model?.Name ?? "Create realm";
}

@if (Model == null)
{
  <realm-edit></realm-edit>
}
else
{
  <realm-edit json="@(ModelSerializer.Serialize(Model))" status="@(Context.Request.Query["status"])"></realm-edit>
}
