@using Portal.Core.Users.Models;

@model UserModel
@{
  Layout = "_Layout";

  ViewBag.Title = Model?.FullName ?? Model?.Username ?? "Create user";
}

@if (Model == null)
{
  <user-edit realm="@(Context.Request.Query["realm"])"></user-edit>
}
else
{
  <user-edit json="@(ModelSerializer.Serialize(Model))" status="@(Context.Request.Query["status"])"></user-edit>
}
